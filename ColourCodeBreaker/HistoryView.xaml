<Window x:Class="ColourCodeBreaker.HistoryView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColourCodeBreaker"
        mc:Ignorable="d"
        Title="HistoryView" Height="500" Width="500" Background="#444444"
        WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid>
        <Label x:Name="label_MovesHistory" Content="Moves history" HorizontalAlignment="Center" Margin="0,15,0,0" VerticalAlignment="Top" FontSize="20" Foreground="LightGray"/>
    </Grid>
</Window>
